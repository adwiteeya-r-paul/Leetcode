class Solution{
    boolean isPalindrome(int x){
    if (x<0){
        return false;
        }
    else{
        String s = Integer.toString(x);
        int size = s.length();
        for (int i = 0; i < size/2; i++){
            if (s.charAt(i) != s.charAt(size - 1 - i)){
                return false;
            }
        }
        return true;

    
    }
    }
}
